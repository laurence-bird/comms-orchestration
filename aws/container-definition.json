[
  {
    "name": "orchestration",
    "image": "@@AWS_ACCOUNT_ID.dkr.ecr.eu-west-1.amazonaws.com/orchestration:@@VERSION",
    "essential": true,
    "memory": 512,
    "environment": [
      {
        "name": "ENV",
        "value": "@@ENV"
      }
    ],
    "portMappings": [
      {
        "containerPort": 8080,
        "hostPort": 0
      }
    ]
  }
]